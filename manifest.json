{
    "name": "Otto Tabs",
    "description" : "Smart tabs management",
    "version": "0.0.1",
    "manifest_version": 2,
    "permissions": ["storage", "tabs"],
    "browser_action": {
      "default_popup": "index.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "commands": {
        "_execute_browser_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "MacCtrl+Shift+F"
          },
          "description": "Opens hello.html"
        }
      }
  }